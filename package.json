{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.65",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "antd": "^4.23.5",
    "axios": "^1.1.2",
    "craco-antd": "^2.0.0",
    "craco-less": "^2.0.0",
    "immer": "^9.0.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.2",
    "react-scripts": "5.0.1",
    "simple-progress-webpack-plugin": "^2.0.0",
    "typescript": "^4.8.4",
    "use-immer": "^0.7.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@craco/craco": "7.0.0-alpha.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss-less": "^6.0.0",
    "prettier": "2.7.1",
    "stylelint": "^14.14.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^29.0.0",
    "webpack-bundle-analyzer": "^4.6.1"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "lint-staged": "lint-staged",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:style": "stylelint --fix \"src/**/*.css\"",
    "lint:less": "stylelint src/**/*.less --fix  --custom-syntax postcss-less",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx ./src && npm run lint:style",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.{css,less,scss}": "npm run lint:style",
    "**/*.{js,jsx,ts,tsx}": "npm run lint:js",
    "**/*.{js,jsx,tsx,ts,css,less,md,json}": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
